<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"

    >

    <ImageView
        android:id="@+id/iv_rightLine"
        android:layout_width="@dimen/x70"
        android:layout_height="@dimen/x40"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/iv_carImage"
        android:src="@drawable/background_line"
        android:visibility="visible"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/iv_carImage"
        android:layout_width="@dimen/x40"
        android:layout_height="@dimen/x40"
        android:layout_alignParentTop="true"
        android:layout_gravity="center_horizontal"
        android:layout_toEndOf="@+id/iv_leftLine"
        android:background="@drawable/circle_unactive"
        android:src="@drawable/business_unactive"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/iv_leftLine"
        android:layout_width="@dimen/x8"
        android:layout_height="@dimen/x40"
        android:layout_alignParentTop="true"
        android:src="@drawable/background_line"
        android:visibility="visible"
        android:layout_alignParentStart="true"
        tools:ignore="ContentDescription" />

    <com.app.fastcab.ui.views.AnyTextView
        android:id="@+id/txt_carTypeName"
        android:layout_width="@dimen/x60"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_carImage"
        android:layout_marginTop="@dimen/x10"
        android:gravity="center_horizontal"
        android:text="@string/business"
        app:typeface="Roboto-Regular.ttf" />


</RelativeLayout>